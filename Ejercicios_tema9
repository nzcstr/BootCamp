public class MyClass {
    public static void main(String args[]) {
      Cliente myClient = new Cliente();
      Trabajador myWorker = new Trabajador();
      
      myClient.edad = 45;
      myClient.nombre = "Rodrigo";
      myClient.telefono = 657263355;
      
      myWorker.edad = 30;
      myWorker.nombre = "Camilo";
      myWorker.telefono = 657263354;
      myWorker.salario = 1838.7;
      
      System.out.println(myClient.edad);
      System.out.println(myClient.nombre);
      System.out.println(myClient.telefono);
      
      System.out.println(myWorker.edad);
      System.out.println(myWorker.nombre);
      System.out.println(myWorker.telefono);
      System.out.println(myWorker.salario);
      
      
    }
}

class Persona {
    int edad;
    String nombre;
    int telefono;
    
}

class Cliente extends Persona {
    float credito;
}

class Trabajador extends Persona {
    double salario;
}
